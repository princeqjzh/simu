		<!-- inline scripts related to this page -->

		<script type="text/javascript">
			function show_box(id) {
			 jQuery('.widget-box.visible').removeClass('visible');
			 jQuery('#'+id).addClass('visible');
			}
		</script>
	<script type="text/javascript">
		jQuery(function($)  {
		$("#txtUser").focus();
		document.onkeydown = function(e){ 
			var ev = (e)?e:((window.event)?window.event:"");
			keyCode = ev.keyCode?ev.keyCode:(ev.which?ev.which:ev.charCode);//兼容所有浏览器
		    if(keyCode==1333) {
		    	login();
		     }
		};
		
	});
	function login() {
		if ($("#txtUser").val() == "") {
			showmsg("用户名称不能为空~~");
			$("#txtUser").focus();
			return false;
		} else if ($("#txtPass").val() == "") {
			showmsg("密码不能为空~~");
			$("#txtPass").focus();
			return false;
		}else{
			//$("#form1").submit();
			var txtPass=$("#txtUser").val();
			var txtPass=$("#txtPass").val();
				 $.ajax({
						type : "POST",
						dataType : "text",
						url : "login.do?method=login&nowdate=" + new Date(),
						data : {
							username : $("#txtUser").val(),
							userpass : $("#txtPass").val(),
						},
						success : function(msg) {
							showmsg(msg);
						}
					});
		}
	}
	function formreset(){
		$("#form1").form("clear");
		$("#txtUser").focus();
	}
	function validateusername() {
		$.ajax({
			type : "POST",
			dataType : "text",
			url : "login.do?method=checkUsername&nowdate=" + new Date(),
			data : {
				username : $("#txtUser").val()
			},
			success : function(msg) {
				showmsg(msg);
			}
		});
	}
	//提示信息
	function showmsg(data){
		$.messager.show({
									title : "提示信息",
									msg : data,
									showType : "show",
									timeout : 3000
								});
	}
	</script>
</body>
</html>
